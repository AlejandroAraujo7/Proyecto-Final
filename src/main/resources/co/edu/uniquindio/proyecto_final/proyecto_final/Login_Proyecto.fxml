<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane
        maxHeight="-Infinity"
        maxWidth="-Infinity"
        minHeight="-Infinity"
        minWidth="-Infinity"
        prefHeight="403.0"
        prefWidth="341.0"
        xmlns="http://javafx.com/javafx/22"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="co.edu.uniquindio.proyecto_final.proyecto_final.viewcontroller.LoginViewcontroller">
    <children>
        <TextField
                fx:id="txtEmail"
                layoutX="65.0"
                layoutY="176.0"
                prefHeight="24.0"
                prefWidth="209.0"
                promptText="Example@yahoo.co" />
        <Label
                layoutX="65.0"
                layoutY="160.0"
                text="Email:" />
        <Label
                layoutX="66.0"
                layoutY="220.0"
                text="Password" />
        <PasswordField
                fx:id="txtContraseña"
                layoutX="65.0"
                layoutY="236.0"
                prefHeight="24.0"
                prefWidth="209.0"
                promptText="******" />
        <Button
                fx:id="btnSingin"
                layoutX="65.0"
                layoutY="274.0"
                mnemonicParsing="false"
                onAction="#onSinginUser"
                prefHeight="24.0"
                prefWidth="82.0"
                text="Sign in" />
        <Button
                fx:id="btnLogin"
                layoutX="192.0"
                layoutY="274.0"
                mnemonicParsing="false"
                onAction="#handleLoginUser"
                prefHeight="24.0"
                prefWidth="82.0"
                text="Log in" />
        <Label
                layoutX="66.0"
                layoutY="317.0"
                prefHeight="24.0"
                prefWidth="174.0"
                text="¿Olvidaste tu contraseña?"
                textAlignment="CENTER"
                underline="true" />
        <ImageView
                fx:id="imageViewLogo"
                fitHeight="119.0"
                fitWidth="142.0"
                layoutX="115.0"
                layoutY="18.0"
                pickOnBounds="true"
                preserveRatio="true" />
    </children>
</AnchorPane>
